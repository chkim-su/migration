      "version": "1.1.0",
      "source": "./",
      "license": "MIT",
      "tags": [
        "solid",
        "migration",
        "audit",
        "tdd",
        "strict",
        "no-dummy"
      ],
      "skills": [
        "./skills/solid-design-rules",
        "./skills/strict-implementation-rules"
      ],
      "commands": [
        "./commands/migrate.md",
        "./commands/analyze.md",
        "./commands/audit.md"
      ],
      "agents": [
        "./agents/solid-analyzer.md",
        "./agents/migration-planner.md",
        "./agents/implementation-auditor.md",
        "./agents/static-test-runner.md",
        "./agents/simulation-test-runner.md",
        "./agents/test-orchestrator.md"
