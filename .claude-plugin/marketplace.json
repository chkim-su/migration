{
  "name": "strict-migration-marketplace",
  "owner": {
    "name": "chanhokim"
  },
  "plugins": [
    {
      "name": "strict-migration",
      "description": "Enforces strict implementation standards during refactoring and migration. Prevents dummy code, hardcoding, and scope creep with real user simulation testing.",
      "version": "1.1.0",
      "source": {
        "type": "github",
        "repo": "chkim-su/migration"
      },
      "license": "MIT",
      "tags": ["solid", "migration", "audit", "tdd", "strict", "no-dummy"],
      "skills": [
        "./skills/solid-design-rules.md",
        "./skills/strict-implementation-rules.md"
      ],
      "commands": [
        "./commands/migrate.md",
        "./commands/analyze.md",
        "./commands/audit.md"
      ],
      "agents": [
        "./agents/solid-analyzer.md",
        "./agents/migration-planner.md",
        "./agents/implementation-auditor.md",
        "./agents/static-test-runner.md",
        "./agents/simulation-test-runner.md",
        "./agents/test-orchestrator.md"
      ],
      "hooks": [
        "./hooks/hooks.json"
      ]
    }
  ]
}
